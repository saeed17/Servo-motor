#include <Servo.h>


Servo ser1;
Servo ser2;
Servo ser3;
Servo ser4;
Servo ser5;

int serpos=0;

void setup()

{
  ser1.attach(2);
  ser2.attach(3);
  ser3.attach(4);
  ser4.attach(5);
  ser5.attach(6);
}
void loop()
{
  
  ser1.write(90);
  ser2.write(90);
  ser3.write(90);
  ser4.write(90);
  ser5.write(90);
}
